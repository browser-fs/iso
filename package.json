{
	"name": "@zenfs/iso",
	"version": "0.1.3",
	"description": "Iso backend for ZenFS",
	"main": "dist/index.js",
	"types": "src/index.ts",
	"keywords": [
		"filesystem",
		"node",
		"storage"
	],
	"type": "module",
	"homepage": "https://github.com/zen-fs/iso",
	"author": "dr-vortex <jp@drvortex.dev>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zen-fs/iso.git"
	},
	"bugs": {
		"url": "https://github.com/zen-fs/iso/issues"
	},
	"engines": {
		"node": ">= 18"
	},
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint src",
		"build": "npx build --package=@zenfs/core --globalName ZenFS_Iso --entry src/index.ts",
		"build:docs": "typedoc --out docs --name 'ZenFS ISO' src/index.ts",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@fal-works/esbuild-plugin-global-externals": "^2.1.2",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"esbuild": "^0.20.2",
		"eslint": "^8.36.0",
		"prettier": "^3.2.5",
		"typedoc": "^0.25.1",
		"typescript": "5.2.2"
	},
	"peerDependencies": {
		"@zenfs/core": "^0.10.0"
	},
	"dependencies": {
		"utilium": "^0.3.4"
	}
}
